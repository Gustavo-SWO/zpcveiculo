<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" controllerName="zpcveiculo.controller.Details" displayBlock="true">
    <Page id="idDetailsPage" title="{i18n>titleDetails}" contentOnlyBusy="true">
    <content>
        <smartForm:SmartForm id="idSmartForm" editTogglable="true" validationMode="Async" editToggled="onSmartFormEditToggled">
                    <smartForm:customToolbar>
                        <Toolbar id="idToolbar" width="100%" height="100%">
                            <ToolbarSpacer id="id1ToolbarSpacer"/>
                            <Button id="idGravarButton" type="Emphasized" text="{i18n>saveBtn}" icon="sap-icon://save" press="onGravarButtonPress" visible="false"/>
                        </Toolbar>
                    </smartForm:customToolbar>
                <smartForm:groups>
                    <smartForm:Group title="{i18n>titleGeneralDataGroup}" id="idGenDataGroup">
                        <smartForm:groupElements>
                            <smartForm:GroupElement id="idVehicleGroupElement" visible="false">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehicleSmartField" value="{Vehicle}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehTextGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehTextSmartField" value="{VeiculoText/VehText}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehTypeGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehTypeSmartField" value="{VehType}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idCarrierGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idCarrierSmartField" value="{Carrier}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                        </smartForm:groupElements>
                    </smartForm:Group>
                    <smartForm:Group title="{i18n>titleTUAttributes}" id="idTUAttributesGroup">
                        <smartForm:groupElements>
                            <smartForm:GroupElement id="idVehLengthGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehLengthSmartField" value="{VehLength}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehWidthGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehWidthSmartField" value="{VehWidth}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehHeightGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehHeightSmartField" value="{VehHeight}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehUnlwgtGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehUnlwgtSmartField" value="{VehUnlwgt}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehMaxwgtGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehMaxwgtSmartField" value="{VehMaxwgt}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                            <smartForm:GroupElement id="idVehMaxvolGroupElement">
                                <smartForm:elements>
                                    <smartField:SmartField id="idVehMaxvolSmartField" value="{VehMaxvol}" entitySet="VeiculoSet"/>
                                </smartForm:elements>
                            </smartForm:GroupElement>
                        </smartForm:groupElements>
                    </smartForm:Group>
                </smartForm:groups>
            </smartForm:SmartForm>
    </content>
</Page>
</mvc:View>